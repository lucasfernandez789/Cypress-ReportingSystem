{
  "name": "cypress-leyes-cypress-reportingsystem",
  "version": "1.0.0",
  "description": "Sistema de reportes Cypress para aplicaciÃ³n de leyes",
  "type": "module",
  "main": "index.js",
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "test": "npm run clean-reports && cypress run",
    "test:core": "npm run clean-reports && cypress run --spec \"cypress/e2e/core/*.cy.js\"",
    "test:features": "npm run clean-reports && cypress run --spec \"cypress/e2e/features/*.cy.js\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run-reports": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json",
    "posttest": "npm run report:merge && npm run report:generate && npm run report:generate-json",
    "posttest:core": "npm run report:merge && npm run report:generate && npm run report:generate-json",
    "posttest:features": "npm run report:merge && npm run report:generate && npm run report:generate-json",
    "report:publish": "node scripts/publish-reports.js",
    "report:merge": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/report.json",
    "report:generate": "npm run report:generate-timestamped",
    "report:sync-docs": "node scripts/sync-reports-to-docs.js",
    "report:generate-timestamped": "node scripts/generate-timestamped-report.js",
    "report:generate-json": "node scripts/generate-reports-json.js cypress/reports docs/reports",
    "delete-report": "node scripts/delete-report.js",
    "api-server": "node scripts/api-server.js",
    "setup": "node scripts/setup.js",
    "setup:env": "node scripts/setup.js --env",
    "setup:tests": "node scripts/setup.js --tests",
    "setup:app": "node scripts/setup.js --app",
    "verify": "node scripts/verify-setup.js",
    "cleanup": "node scripts/cleanup-template.js",
    "clean-reports": "node scripts/clean-reports.js",
    "create:template": "node scripts/create-template.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "cors": "^2.8.5",
    "cypress": "^15.3.0",
    "cypress-multi-reporters": "^2.0.5",
    "dotenv": "^17.2.2",
    "eslint": "^9.37.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "express": "^5.1.0",
    "fs-extra": "^11.3.2",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "vite": "^7.1.9"
  }
}
