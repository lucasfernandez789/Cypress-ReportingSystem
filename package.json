{
  "name": "cypress-leyes",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "test": "npm run clean-reports && cypress run",
    "test:core": "npm run clean-reports && cypress run --spec 'cypress/e2e/core/**/*'",
    "test:features": "npm run clean-reports && cypress run --spec 'cypress/e2e/features/**/*'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run-reports": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json",
    "posttest": "npm run report:merge && npm run report:generate && npm run report:sync-docs",
    "posttest:core": "npm run report:merge && npm run report:generate && npm run report:sync-docs",
    "posttest:features": "npm run report:merge && npm run report:generate && npm run report:sync-docs",
    "report:merge": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/report.json",
    "report:generate": "npm run report:generate-timestamped",
    "report:sync-docs": "node scripts/sync-reports-to-docs.js",
    "report:generate-timestamped": "node scripts/generate-timestamped-report.js",
    "delete-report": "node scripts/delete-report.js",
    "api-server": "node scripts/api-server.js",
    "setup:app": "node scripts/setup-app.js",
    "setup:env": "node scripts/setup-env.js",
    "setup:tests": "node scripts/setup-tests.js",
    "setup": "npm run setup:app && npm run setup:env && npm run setup:tests",
    "verify": "node scripts/verify-setup.js",
    "cleanup": "node scripts/cleanup-template.js",
    "clean-reports": "node scripts/clean-reports.js",
    "report:history": "Get-ChildItem public/reports/20*/report-*.html | Sort-Object LastWriteTime -Descending | Select-Object -First 20"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "cors": "^2.8.5",
    "cypress": "^15.3.0",
    "cypress-multi-reporters": "^2.0.5",
    "dotenv": "^17.2.2",
    "eslint": "^9.37.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "express": "^5.1.0",
    "fs-extra": "^11.3.2",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "vite": "^7.1.9"
  }
}
